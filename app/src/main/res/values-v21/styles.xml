<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LaunchActivityTheme" parent="AppTheme">
        <item name="android:statusBarColor">@android:color/transparent</item>

    </style>
    <style name="MainActivityTheme" parent="AppTheme">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="colorPrimaryDark">#d33a31</item>
    </style>

    <style name="LocalMusicTheme" parent="AppTheme">
        <item name="android:statusBarColor">#d33a31</item>
        <item name="colorPrimaryDark">#d33a31</item>
    </style>
    <style name="MusicActivityTheme" parent="AppTheme">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="SerchActivityTheme" parent="AppTheme">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>




</resources>